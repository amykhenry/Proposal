@article{Moore:2010ex,
author = {Moore, Sean M. and Borer, Elizabeth T. and Hosseini, Parviez R.},
title = {{Predators indirectly control vector-borne disease: linking predator-prey and host-pathogen models}},
journal = {Journal of the Royal Society Interface},
year = {2010},
volume = {7},
number = {42},
pages = {161--176},
affiliation = {Oregon State Univ, Dept Zool, Corvallis, OR 97331 USA},
doi = {10.1098/rsif.2009.0131},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-06-04T19:19:05GMT+00:00},
date-modified = {2013-01-24T20:07:21GMT+00:00},
abstract = {Pathogens transmitted by arthropod vectors are common in human populations, agricultural systems and natural communities. Transmission of these vector-borne pathogens depends on the population dynamics of the vector species as well as its interactions with other species within the community. In particular, predation may be sufficient to control pathogen prevalence indirectly via the vector. To examine the indirect effect of predators on vectored-pathogen dynamics, we developed a theoretical model that integrates predator-prey and host-pathogen theory. We used this model to determine whether predation can prevent pathogen persistence or alter the stability of host-pathogen dynamics. We found that, in the absence of predation, pathogen prevalence in the host increases with vector fecundity, whereas predation on the vector causes pathogen prevalence to decline, or even become extinct, with increasing vector fecundity. We also found that predation on a vector may drastically slow the initial spread of a pathogen. The predator can increase host abundance indirectly by reducing or eliminating infection in the host population. These results highlight the importance of studying interactions that, within the greater community, may alter our predictions when studying disease dynamics. From an applied perspective, these results also suggest situations where an introduced predator or the natural enemies of a vector may slow the rate of spread of an emerging vector-borne pathogen.},
url = {http://171.66.127.193/content/7/42/161.short},
local-url = {file://localhost/Volumes/LaCie/Papers2/Articles/2010/Moore/J%20R%20Soc%20Interface%202010%20Moore.pdf},
file = {{J R Soc Interface 2010 Moore.pdf:/Volumes/LaCie/Papers2/Articles/2010/Moore/J R Soc Interface 2010 Moore.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1098/rsif.2009.0131}}
}

