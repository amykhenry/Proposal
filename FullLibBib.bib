@article{Smith:2009ee,
author = {Smith, Katherine F and Behrens, Michael D. and Sax, Dov F},
title = {{Local scale effects of disease on biodiversity.}},
journal = {EcoHealth},
year = {2009},
volume = {6},
number = {2},
pages = {287--295},
month = jun,
affiliation = {Brown University, Ecology and Evolutionary Biology, Providence, RI 02912, USA. Katherine{\_}Smith@brown.edu},
doi = {10.1007/s10393-009-0254-9},
pmid = {19921487},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-10-22T22:31:13GMT+00:00},
date-modified = {2013-01-24T21:35:08GMT+00:00},
abstract = {To date, ecologists and conservation biologists have focused much of their attention on the population and ecosystem effects of disease at regional scales and the role that diseases play in global species extinction. Far less research has been dedicated to identifying the effects that diseases can have on local scale species assemblages. We examined the role of infectious disease in structuring local biodiversity. Our intention was to illustrate how variable outcomes can occur by focusing on three case studies: the influence of chestnut blight on forest communities dominated by chestnut trees, the influence of red-spot disease on urchin barrens and kelp forests, and the influence of sylvatic plague on grassland communities inhabited by prairie dogs. Our findings reveal that at local scales infectious disease seems to play an important, though unpredictable, role in structuring species diversity. Through our case studies, we have shown that diseases can cause drastic population declines or local extirpations in keystone species, ecosystem engineers, and otherwise abundant species. These changes in local diversity may be very important, particularly when considered alongside potentially corresponding changes in community structure and function, and we believe that future efforts to understand the importance of disease to species diversity should have an increased focus on these local scales.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=19921487&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Volumes/LaCie/Papers2/Articles/2009/Smith/Ecohealth%202009%20Smith.pdf},
file = {{Ecohealth 2009 Smith.pdf:/Volumes/LaCie/Papers2/Articles/2009/Smith/Ecohealth 2009 Smith.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1007/s10393-009-0254-9}}
}

